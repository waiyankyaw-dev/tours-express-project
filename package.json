    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.3",
    "helmet": "^4.1.1",
    "hpp": "^0.2.3",
    "html-to-text": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
